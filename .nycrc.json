{
    "extends": "@istanbuljs/nyc-config-typescript",
    "check-coverage": true,
    "extension": [
        ".ts"
    ],
    "exclude": [
        "src/**/*.spec.ts"
    ],
    "include": [
        "src/**/*.ts"
    ],
    "reporter": [
        "text"
    ],
    "branches": 80,
    "lines": 80,
    "functions": 80,
    "statements": 80
}
